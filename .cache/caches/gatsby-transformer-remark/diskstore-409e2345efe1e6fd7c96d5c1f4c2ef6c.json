ildren":[{"type":"text","value":"\"Accessibility supported\" relates to support by user agents (including assistive technologies) of particular ways of using Web technologies. Uses of Web technologies that are accessibility supported will work with assistive technologies and access features in mainstream user agents (browsers and players etc.).","position":{"start":{"line":60,"column":1,"offset":4816},"end":{"line":60,"column":312,"offset":5127},"indent":[]}}],"position":{"start":{"line":60,"column":1,"offset":4816},"end":{"line":60,"column":312,"offset":5127},"indent":[]}},{"type":"paragraph","children":[{"type":"strong","children":[{"type":"text","value":"Note","position":{"start":{"line":62,"column":3,"offset":5131},"end":{"line":62,"column":7,"offset":5135},"indent":[]}}],"position":{"start":{"line":62,"column":1,"offset":5129},"end":{"line":62,"column":9,"offset":5137},"indent":[]}},{"type":"text","value":": Both Section 508 and WCAG require electronic stuff to ‘Work with AT’.","position":{"start":{"line":62,"column":9,"offset":5137},"end":{"line":62,"column":80,"offset":5208},"indent":[]}}],"position":{"start":{"line":62,"column":1,"offset":5129},"end":{"line":62,"column":80,"offset":5208},"indent":[]}}],"position":{"start":{"line":1,"column":1,"offset":0},"end":{"line":63,"column":1,"offset":5209}}}}